{
    "Carrusel CSS": {
    "prefix": "carrusel_css",
    "body": [
        "#carrusel {",
        "\tfloat:left;",
        "\twidth:600px;",
        "\toverflow:hidden;",
        "\theight:203px;",
        "\tposition:relative;",
        "\tmargin-top:20px;",
        "\tmargin-bottom:20px;",
        "}",
        "#carrusel .left-arrow {",
        "\tposition:absolute;",
        "\tleft:10px;",
        "\tz-index:1;",
        "\ttop:50%;",
        "\tmargin-top:-9px;",
        "}",
        "#carrusel .right-arrow {",
        "\tposition:absolute;",
        "\tright:10px;",
        "\tz-index:1;",
        "\ttop:50%;",
        "\tmargin-top:-9px;",
        "}",
        ".carrusel {",
        "\twidth:4000px;",
        "\tleft:0px;",
        "\tposition:absolute;",
        "\tz-index:0;",
        "}",
        ".carrusel>div {",
        "\tfloat: left;",
        "\theight: 203px;",
        "\tmargin-right: 5px;",
        "\twidth: 195px;",
        "\ttext-align:center;",
        "}",
        ".carrusel img {",
        "\tcursor:pointer;",
        "}",
        ".product {",
        "\tborder:#CCCCCC 1px solid;",
        "}"
    ],
    "description": "carrusel style"
    }
    }